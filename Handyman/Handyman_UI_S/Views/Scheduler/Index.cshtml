@{ List<string> viewCollection = new List<string>();
       viewCollection.Add("Day");
       viewCollection.Add("Week");
       viewCollection.Add("WorkWeek");
       viewCollection.Add("Month");
       viewCollection.Add("Agenda");
       viewCollection.Add("MonthAgenda");
       viewCollection.Add("TimelineDay");
       viewCollection.Add("TimelineWeek");
       viewCollection.Add("TimelineWorkWeek");
       viewCollection.Add("TimelineMonth");
}
@(Html.EJS().Schedule("schedule")
            .Width("100%")
            .Height("550px")
			.TimeScale(ts => ts.Interval(60).SlotCount(6)).Views(viewCollection)
            .EventSettings(es => es
            .Fields(fs => fs
                    .Id("Id")
                    .Subject(opt => opt.Name("Subject"))
                    .StartTime(opt => opt.Name("StartTime"))
                    .EndTime(opt => opt.Name("EndTime"))
                    .IsAllDay(opt => opt.Name("IsAllDay"))
                 )
                 .DataSource(ViewBag.dataSource)
             )
             .SelectedDate(new DateTime(2017, 6, 15))
             .Render())

