<Project Sdk="Microsoft.NET.Sdk.Web">

  <PropertyGroup>
    <TargetFramework>net7.0</TargetFramework>
    <Nullable>enable</Nullable>
    <ImplicitUsings>enable</ImplicitUsings>
    <RootNamespace>Handyman_SP_UI</RootNamespace>
    <UserSecretsId>f7f65db5-005e-4030-80a8-0fb3eaa8b861</UserSecretsId>
  </PropertyGroup>

  <ItemGroup>
    <Compile Remove="Models\ServiceProvider\**" />
    <Content Remove="Models\ServiceProvider\**" />
    <EmbeddedResource Remove="Models\ServiceProvider\**" />
    <None Remove="Models\ServiceProvider\**" />
  </ItemGroup>

  <ItemGroup>
    <Content Remove="Pages\Businesses\BizCreate.razor" />
    <Content Remove="Pages\Businesses\NewBusiness.razor" />
    <Content Remove="Pages\Businesses\Requests\Tasks\CancelledRequestsComponent.razor" />
    <Content Remove="Pages\ServiceProviderComponent\ProviderProfile.razor" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.AspNetCore.Diagnostics.EntityFrameworkCore" Version="7.0.2" />
    <PackageReference Include="Microsoft.AspNetCore.Identity.EntityFrameworkCore" Version="7.0.0" />
    <PackageReference Include="Microsoft.AspNetCore.Identity.UI" Version="7.0.0" />
    <PackageReference Include="Microsoft.EntityFrameworkCore.SqlServer" Version="7.0.0" />
    <PackageReference Include="Microsoft.EntityFrameworkCore.Tools" Version="7.0.0">
      <PrivateAssets>all</PrivateAssets>
      <IncludeAssets>runtime; build; native; contentfiles; analyzers; buildtransitive</IncludeAssets>
    </PackageReference>
    <PackageReference Include="Microsoft.VisualStudio.Web.CodeGeneration.Design" Version="7.0.0" />
    <PackageReference Include="MSTest" Version="3.0.2" />
    <PackageReference Include="NUnit" Version="3.13.3" />
  </ItemGroup>



  <ItemGroup>
    <None Include="Pages\Businesses\Requests\CancelledRequestsComponent.razor" />
    <None Include="Pages\ServiceProviderComponent\Profile\ProviderProfile.razor" />
    <None Include="Pages\Shared\NewBusiness.razor" />
  </ItemGroup>



  <ItemGroup>
    <ProjectReference Include="..\HandymanProviderLibrary\HandymanProviderLibrary.csproj" />
  </ItemGroup>



  <ItemGroup>
    <Content Update="Pages\EmployeeComponent\Employees.razor">
      <ExcludeFromSingleFile>true</ExcludeFromSingleFile>
    </Content>
    <Content Update="wwwroot\assets\dist\css\blog.css">
      <ExcludeFromSingleFile>true</ExcludeFromSingleFile>
      <CopyToPublishDirectory>PreserveNewest</CopyToPublishDirectory>
    </Content>
    <Content Update="wwwroot\assets\dist\css\headers.css">
      <ExcludeFromSingleFile>true</ExcludeFromSingleFile>
      <CopyToPublishDirectory>PreserveNewest</CopyToPublishDirectory>
    </Content>
  </ItemGroup>



  <ItemGroup>
    <UpToDateCheckInput Remove="Pages\Businesses\BizCreate.razor" />
  </ItemGroup>



  <ItemGroup>
    <_ContentIncludedByDefault Remove="Pages\Businesses\BizCreate.razor" />
    <_ContentIncludedByDefault Remove="Pages\Businesses\Requests\Tasks\CancelledRequestsComponent.razor" />
    <_ContentIncludedByDefault Remove="Pages\ServiceProviderComponent\ProviderProfile.razor" />
  </ItemGroup>



  <ItemGroup>
    <UpToDateCheckInput Remove="Pages\ServiceProviderComponent\ProviderProfile.razor" />
  </ItemGroup>



  <ItemGroup>
    <UpToDateCheckInput Remove="Pages\Businesses\Requests\Tasks\CancelledRequestsComponent.razor" />
  </ItemGroup>

</Project>
